		 <clase expresion="0 0 9 1/1 * ? *"     id="rfc"         description="Validar los rfc SAT"        load="false"  desarrollo="*" pruebas="*" produccion="*">mx.org.kaana.jobs.Rfcs</clase>
		 <clase expresion="0 0/7 5-20 * * ? *"  id="conteos"     description="Conteos remotos"            load="true"   desarrollo="*" pruebas="*" produccion="*">mx.org.kaana.jobs.Conteos</clase>
