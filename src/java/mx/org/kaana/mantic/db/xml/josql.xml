<?xml version="1.0" encoding="ISO-8859-1"?>
<!--
		Relación de consultas para las vistas de MANTIC
-->
<process>
	<dml> 
		<unit id="Domicilio">
            <select id="row">
              select  
                * 
              from 
                mx.org.kaana.mantic.catalogos.proveedores.beans.Domicilio
              where 
                accion = '{accion}';
            </select>  
        </unit>  
            
                    
	</dml>    
</process>
